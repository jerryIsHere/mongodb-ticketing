{
    "version": 2,
    "installCommand": "npm install && npm install --prefix web",
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "builds": [
        {
            "src": "/build/index.js",
            "use": "@vercel/node" 
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/build/index.js"
        }
    ]
}

{
    "version": 2,
    "installCommand": "npm install && npm install --prefix web",
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/build/index.js"
        }
    ],
    "functions": {
        "build/index.js": {
            "memory": 3009,
            "maxDuration": 30
        }
    }
}

{
    "version": 2,
    "builds": [
        {
            "src": "/api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "web/dist/ticketing/browser/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/web/(.*)",
            "dest": "/web/dist/ticketing/browser/$1"
        },
        {
            "src": "/web/(.*)",
            "status": 404,
            "dest": "/web/dist/ticketing/browser/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ]
}